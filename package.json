{
  "name": "@veterancrowd/aws-service",
  "version": "1.24.7",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veterancrowd/aws-service"
  },
  "author": "Jason G. Williscroft",
  "bugs": {
    "url": "https://github.com/veterancrowd/aws-service/issues"
  },
  "description": "AWS service utilities.",
  "homepage": "https://github.com/veterancrowd/aws-service#readme",
  "keywords": [
    "aws"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.332.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.332.0",
    "@aws-sdk/client-sfn": "^3.332.0",
    "@karmaniverous/edge-logger": "^1.2.0",
    "@middy/core": "^4.4.1",
    "@middy/http-cors": "^4.4.1",
    "@middy/http-error-handler": "^4.4.1",
    "@middy/http-event-normalizer": "^4.4.1",
    "@middy/http-json-body-parser": "^4.4.1",
    "@middy/http-response-serializer": "^4.4.1",
    "@middy/validator": "^4.4.1",
    "@sentry/serverless": "^7.51.2",
    "ajv": "^8.12.0",
    "boolean": "^3.2.0",
    "commander": "^10.0.1",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "openapi-client-axios": "^7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@karmaniverous/get-dotenv": "^2.3.4",
    "@types/node": "^20.1.3",
    "chai": "^4.3.7",
    "concat-md": "^0.5.1",
    "eslint": "^8.40.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.3",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-yml": "^1.7.0",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "mocha": {
    "exclude": [
      "./node_modules/**"
    ],
    "file": "./test/setup.js",
    "require": [
      "@babel/register"
    ],
    "spec": "./**/*.test.!(*.*)",
    "timeout": 10000
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "scripts": {
    "doc": "jsdoc2md -c doc/jsdoc.config.json -f lib/**/*.* -t doc/api-template.hbs > doc/3-api.jsdoc2.md && concat-md doc --hide-anchor-links > README.md",
    "lint": "eslint lib/**",
    "package": "npm run lint && npm run test && npm run doc",
    "release": "npm run package && getdotenv -- release-it",
    "test": "getdotenv -c \"mocha\" -p ./ ./env -d dev -y ./env/dynamic.js"
  },
  "type": "module"
}
